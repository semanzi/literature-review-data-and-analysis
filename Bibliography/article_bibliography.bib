@InProceedings{Karim2022,
  author    = {Karim, Shakir and Uddin, Shahadat},
  title     = {Analysis of Patient-Sharing Provider Network in Macarthur Region, New South Wales, Australia Using Administrative Claim Data},
  booktitle = {Proceedings of the 2022 Australasian Computer Science Week},
  year      = {2022},
  series    = {ACSW '22},
  pages     = {207–215},
  address   = {New York, NY, USA},
  publisher = {Association for Computing Machinery},
  abstract  = {Past investigations have recorded the utilisation of administrative claim data for medical care research purposes. Also, administrative claim data uncovers massive professional networks among medical services providers, such as information and knowledge sharing. During care for patients, the connection among providers makes a considerable commitment to efficient health results. Patient-sharing provider networks (PSN) as a joint effort are new and analysed to investigate their effect on medical services results; the impact of such collaborative efforts is yet unidentified. This study aims to analyse PSN for different suburbs of the Macarthur region in New South Wales (NSW), Australia and explore health care collaboration between patients and providers. It has illustrated the usefulness of analysing this network and its applications in studying the collaborative efforts of patients and different healthcare service providers. It also has discussed patient satisfaction as it is one of the several ways to evaluate health service quality, composing an effective indicator to measure providers' success. In this context, determining the main factors contributing to patient satisfaction is crucial to improving quality in healthcare delivery. The corresponding literature suggests that socio-demographic characteristics play a vital role in patient satisfaction. Therefore, based on the social network analysis methodology, this paper measures the connections between gender, age, family status, level of education, incomes, living quality, and patient satisfaction in the Macarthur region. The findings and insights from this research can potentially help healthcare stakeholders formulate better policies, eventually improving the quality of care. The analyses conducted in this study illustrate the potential for using network analysis to provide new insights into the Australian healthcare system and opportunities for prompting future improvements.},
  doi       = {10.1145/3511616.3513117},
  isbn      = {9781450396066},
  keywords  = {Social network analysis, Patient-sharing provider network, Macarthur region, Administrative data},
  location  = {Brisbane, Australia},
  numpages  = {9},
  url       = {https://doi.org/10.1145/3511616.3513117},
}

