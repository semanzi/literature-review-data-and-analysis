@Article{Uddin2016a,
  author   = {Uddin, Shahadat and Khan, Arif and Piraveenan, Mahendra},
  title    = {A set of measures to quantify the dynamicity of longitudinal social networks},
  journal  = {Complexity},
  year     = {2016},
  volume   = {21},
  number   = {6},
  pages    = {309-320},
  abstract = {This study proposes a new set of measures for longitudinal social networks (LSNs). A LSN evolves over time through the creation and/or deletion of links among a set of actors (e.g., individuals or organizations). The current literature does feature some methods, such as multiagent simulation models, for studying the dynamics of LSNs. These methods have mainly been utilized to explore evolutionary changes in LSNs from one state to another and to explain the underlying mechanisms for these changes. However, they cannot quantify different aspects of a LSN. For example, these methods are unable to quantify the level of dynamicity shown by an actor in a LSN and its contribution to the overall dynamicity shown by that LSN. This article develops a set of measures for LSNs to overcome this limitation. We illustrate the benefits of these measures by applying them to an exploration of the Enron crisis. These measures successfully identify a significant but previously unobserved change in network structures (both at individual and group levels) during Enron's crisis period. © 2015 Wiley Periodicals, Inc. Complexity 21: 309–320, 2016},
  doi      = {https://doi.org/10.1002/cplx.21690},
  eprint   = {https://onlinelibrary.wiley.com/doi/pdf/10.1002/cplx.21690},
  keywords = {network dynamicity, longitudinal social network, positional dynamicity, participation dynamicity, complete dynamicity},
  url      = {https://onlinelibrary.wiley.com/doi/abs/10.1002/cplx.21690},
}

